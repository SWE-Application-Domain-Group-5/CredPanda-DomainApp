@model IEnumerable<EliApp.Areas.Identity.Data.EliAppUser>
@{
    ViewData["Title"] = "Index";
}

<h1>List of Users</h1>
<a asp-action="Create">Create</a>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>UserName</td>
            <td>Email</td>
            <td>First Name</td>
            <td>Last Name</td>
            <td>Address</td>
            <td>Date of Birth</td>
            <td>Date Registered</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.GeneratedUserName</td>
                <td>@user.Email</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Address</td>
                <td>@user.DOB</td>
                <td>@user.RegisterDate</td>
                <td>@user.expireDate</td>
            </tr>
        }
    </tbody>
</table>